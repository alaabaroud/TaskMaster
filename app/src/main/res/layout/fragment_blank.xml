        />

    <TextView
        android:id="@+id/body"
        android:layout_width="150dp"
        android:layout_height="101dp"
        android:text="@string/string_body" />


    <TextView
        android:id="@+id/state"
        android:layout_width="429dp"
        android:layout_height="331dp"
        android:text="@string/string_string_state" />
</FrameLayout>